  "engines" : {
    "node" : ">=18.0.0",
    "npm": "8.19.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "bootstrap": "5.3.3",
    "css-loader": "6.11.0",
    "file-loader": "6.2.0",
    "formik": "2.4.6",
    "isomorphic-fetch": "3.0.0",
    "loglevel": "1.9.1",
    "react": "17.0.2",
    "react-bootstrap": "1.6.8",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.1",
    "react-intl": "6.6.8",
    "react-router-dom": "5.3.4",
    "style-loader": "3.3.4",
    "yup": "0.32.11"
  },
  "scripts": {
    "clean": "shx rm -rf build & shx rm -rf dist",
    "compile": "tsc && set NODE_OPTIONS=--openssl-legacy-provider && webpack --mode production --config dist/webpack.prod.js",
    "start": "tsc && set NODE_OPTIONS=--openssl-legacy-provider && webpack serve --mode development --config dist/webpack.config.js",
    "test": "jest --coverage --config jest.config.ts",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "sonarqube": "./node_modules/.bin/sonar-scanner -Dsonar.login=$SONAR_CLOUD_TOKEN",
    "dependency-tree": "npm ls -a > dependencyTree.txt",
    "update": "npx dependency-time-machine --update --install --auto",
    "auditFix": "npm audit fix"
  },
  "devDependencies": {
    "@testing-library/react": "12.1.5",
    "@types/chai": "4.3.16",
    "@types/dotenv-webpack": "7.0.3",
    "@types/fetch-mock": "7.3.8",
    "@types/isomorphic-fetch": "0.0.39",
    "@types/jest": "29.5.12",
    "@types/node": "18.15.3",
    "@types/react": "17.0.80",
    "@types/react-dom": "17.0.25",
    "@types/react-router-dom": "5.3.3",
    "@types/sinon": "5.0.7",
    "@types/webpack-env": "1.16.2",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "chai": "4.4.1",
    "copy-webpack-plugin": "9.0.1",
    "dotenv": "10.0.0",
    "dotenv-webpack": "7.0.3",
    "es7-sleep": "1.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.34.2",
    "fetch-mock": "5.13.1",
    "history": "4.10.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "15.0.0",
    "jest-localstorage-mock": "2.4.26",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "2.8.8",
    "raf": "3.4.1",
    "shx": "0.3.4",
    "sinon": "11.1.2",
    "source-map-loader": "3.0.2",
    "ts-node": "10.9.2",
    "ts-jest": "29.1.4",
    "ts-loader": "9.5.1",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.3.0"
  },
  "proxy": "http://localhost:8080",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jestSonar": {
    "reportPath": "reports/test",
    "reportFile": "test-report.xml"
  }
}